<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.CanteenMapper">

<!--通过餐厅名称查找餐厅id-->
    <select id="selectRcidByRcname" resultType="java.lang.Integer">
        select rcid from canteen where rcname=#{rcname};
    </select>
</mapper>